{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "baseUrl": ".",
    "paths": {
      "@/*": ["client/src/*"],
      "@assets/*": ["attached_assets/*"]
    },
    "types": ["vite/client"],
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "client/src/**/*",
    "vite.config.ts"
  ],
  "exclude": ["node_modules"]
}
